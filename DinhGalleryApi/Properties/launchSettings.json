    "dinhgallery-api": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:58086;http://localhost:58087"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
